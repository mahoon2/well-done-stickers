rules_version = '2';

service cloud.firestore {
  match /databases/{database}/documents {
    match /stickers/{stickerId} {
      // Anyone can view the stickers
      allow read: if true;

      // Only whitelisted users can create, update, or delete
      allow write: if request.auth != null && request.auth.token.email in [
        'mahoon268@gmail.com',
      ];
    }
  }
}